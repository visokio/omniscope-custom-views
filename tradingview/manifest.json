{
  "name": "TradingView",
  "frameworkVersion": "v1",
  "icon": "icon.svg",
  "thumbnail": "thumbnail.png",
  "tags": [],
  "sandbox": false,
  "autoPane": true,
  "autoQuery": true,
  "dataLimit": 10000,
  "supportsSelection": true,
  "options": {
    "items": {
      "aticker": {
        "displayName": "Ticker",
        "documentation": "The field with a function - typically Singleton - which contains the stock ticker (e.g. AAPL).",
        "type": "MEASURE",
        "mandatory": true,
        "validMeasureResultTypes": [
          "TEXT"
        ]
      },
      "details": {
        "displayName": "Show details",
        "documentation": "Show an extra panel with security key stats, like earnings, financial statements, etc.\n N.B. the panel is hidden when the view is too small.",
        "type": "TOGGLE",
        "choicesLayout": "Compact"
      },
      "o_darkMode": {
        "displayName": "Dark mode",
        "type": "TOGGLE",
        "choicesLayout": "Compact"
      },
      "paneX": {
        "displayName": "Pane X",
        "type": "GROUPING",
        "list": true
      },
      "paneY": {
        "displayName": "Pane Y",
        "type": "GROUPING",
        "list": true
      }
    },
    "structure": {
      "toolbar": ["aticker"],
      "x": [
        "paneX"
      ],
      "y": [
        "paneY"
      ]
    }
  },
  "defaultOptions": {
    "items": {},
    "pane": {
      "yAxisHeaderWidth": 75,
      "paneWidth": 200,
      "paneHeight": 200,
      "showGridLines": true,
      "gridLineThickness": 1,
      "gridLineColour": "rgba(229,229,229,1)",
      "headerStyle": {
        "size": 12,
        "colour": "#333333",
        "backgroundColour": "#FFFFFF"
      },
      "xHeaderStyle": {},
      "yHeaderStyle": {},
      "tileHeaderStyle": {},
      "xHeaderPlacement": "TOP",
      "yHeaderPlacement": "LEFT",
      "showColourKeyInEveryPane": false,
      "enableTouchScrolling": false
    }
  },
  "pane": {
    "minWidth": 400,
    "minHeight": 400
  }
}
